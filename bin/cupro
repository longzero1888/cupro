#!/usr/bin/env node
'use strict';
var program = require('commander');
var environment = require('../lib/environment');
var pkg = require('../package.json');

program
    .version(pkg.version)
    .option('-v, --versions', 'check version')
    .parse(process.argv);

if (program.versions) {
    console.log(pkg.version);
} else {
    environment.lookup(program.args);
}


